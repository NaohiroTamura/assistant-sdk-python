{
    "manifest": {
        "displayName": "Commit count report",
        "invocationName": "Commit count report",
        "category": "PRODUCTIVITY"
    },
    "actions": [
        {
            "name": "io.github.naohirotamura.commands.ReportTemperature",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "io.github.naohirotamura.commands.ReportTemperature",
                "trigger": {
                    "queryPatterns": [
                        "Report room temperature"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Reporting room temperature"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "io.github.naohirotamura.commands.ReportTemperature"
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "io.github.naohirotamura.commands.ReportPressure",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "io.github.naohirotamura.commands.ReportPressure",
                "trigger": {
                    "queryPatterns": [
                        "Report pressure"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Reporting pressure"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "io.github.naohirotamura.commands.ReportPressure"
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "com.fujitsu.commands.CommitCountReport",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.fujitsu.commands.CommitCountReport",
                "parameters": [
                    {
                        "name": "from",
                        "type": "SchemaOrg_Date"
                    },
                    {
                        "name": "to",
                        "type": "SchemaOrg_Date"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "query faasshell from $SchemaOrg_Date:from (to $SchemaOrg_Date:to)?",
                        "query github"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Querying faasshell"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.fujitsu.commands.CommitCountReport",
                                    "params": {
                                        "from": "$from",
                                        "to": "$to"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        }
    ]
}
